year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{LOLA}: Enrichment analysis for genomic region sets and regulatory elements",
         author = c(person("Nathan", "Sheffield"),
                    person("Christoph", "Bock")),
         year = year,
         note = note,
         url = "http://lola.databio.org")
